        <arguments>
          <argument>
            <kind>1</kind>
            <string>//checks the keypress to define movement
if (press_up &amp;&amp; !press_down &amp;&amp; !press_right &amp;&amp; !press_left) {
    direction = 90;
    friction = 0;
    if(speed &lt; player_speed) speed += 2;
    else speed = player_speed;
} else if (press_up &amp;&amp; !press_down &amp;&amp; press_right &amp;&amp; !press_left) {
    direction = 45;
    friction = 0;
    if(speed &lt; player_speed) speed += 2;
    else speed = player_speed;
} else if (!press_up &amp;&amp; !press_down &amp;&amp; press_right &amp;&amp; !press_left){
    direction = 0;
    friction = 0;
    if(speed &lt; player_speed) speed += 2;
    else speed = player_speed;
} else if (!press_up &amp;&amp; press_down &amp;&amp; press_right &amp;&amp; !press_left){
    direction = 315;
    friction = 0;
    if(speed &lt; player_speed) speed += 2;
    else speed = player_speed;
} else if (!press_up &amp;&amp; press_down &amp;&amp; !press_right &amp;&amp; !press_left){
    direction = 270;
    friction = 0;
    if(speed &lt; player_speed) speed += 2;
    else speed = player_speed;
} else if (!press_up &amp;&amp; press_down &amp;&amp; !press_right &amp;&amp; press_left){
    direction = 225;
    friction = 0;
    if(speed &lt; player_speed) speed += 2;
    else speed = player_speed;
} else if (!press_up &amp;&amp; !press_down &amp;&amp; !press_right &amp;&amp; press_left){
    direction = 180;
    friction = 0;
    if(speed &lt; player_speed) speed += 2;
    else speed = player_speed;
} else if (press_up &amp;&amp; !press_down &amp;&amp; !press_right &amp;&amp; press_left){
    direction = 135;
    friction = 0;
    if(speed &lt; player_speed) speed += 2;
    else speed = player_speed;
} else friction = 2;

//deals with the shooting sfx
sfx1.x = x-13;
sfx2.x = x+13;
sfx1.y = y-20;
sfx2.y = y-20;
if (sfx1.image_index == 8) sfx1.image_speed = 0;
if (sfx2.image_index == 8) sfx2.image_speed = 0;

//limits the movement
if (x &lt; global.border_left) x = global.border_left;
else if (x &gt; global.border_right) x = global.border_right;
if (y &lt; 32) y = 32;
else if (y &gt; 688) y = 688;

//handles invul_frames
if (global.invul_counter &gt; 0){
    global.invul_counter--;
    if(global.invul_counter%2==1) image_alpha = 0;
    else image_alpha = 1;
}

//handles weapon cooldown
if (!is_firing){
    if(global.player_weapon_heat &gt; 0)
        global.player_weapon_heat -= player_weapon_cooldown_speed;
    else global.player_weapon_heat = 0;
    if (global.player_weapon_heat == 0){
        global.is_overheat = false;
    }
}

//handles weapon firing
if (press_z){
    if (!global.is_overheat){
        cont_to_fire++;
        is_firing = true;
        if (cont_to_fire &gt;= 5) {
            bullet1 = instance_create(x-13,y-20,obj_bullet);    
            bullet2 = instance_create(x+13,y-20,obj_bullet);    
            bullet1.direction = 90;
            bullet2.direction = 90;
            bullet1.speed = 30;
            bullet2.speed = 30;
            sfx1.image_index = 0;
            sfx2.image_index = 0;
            sfx1.image_speed = 1;
            sfx2.image_speed = 1;
            global.player_weapon_heat += player_weapon_heat_speed;
            if (global.player_weapon_heat &gt; global.player_weapon_max_heat){
                global.player_weapon_heat = global.player_weapon_max_heat;
                global.is_overheat = true;
            }
            cont_to_fire = 0;
        }
    }
    else {
        is_firing = false;
    }
}
else is_firing = false;
</string>
          </argument>
        </arguments>
